{
  "name": "txt_tocfill",
  "version": "0.4.0",
  "description": "Text TOC-fill algorithm implementation",
  "main": "build/txt_tocfill.js",
  "scripts": {
    "test": "ava test/*.js",
    "test-verbose": "ava test/*.js -v",
    "clean": "rimraf build -f && rimraf dist -f",
    "babel": "babel src/ -d build/",
    "rename": "node -e \"require('fs').renameSync('./build/txt_tocfill.js', './build/txt_tocfill.es5.js');\"",
    "pack": "browserify -r ./build/txt_tocfill.es5.js:txt_tocfill -o build/txt_tocfill.es5.cjs.js",
    "make": "npm run clean && npm run babel && npm run rename && npm run pack",
    "build": "npm run make && npm run test-verbose",
    "postinstall": "npm run build",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StoneCypher/txt_tocfill.git"
  },
  "keywords": [
    "text",
    "table",
    "of",
    "contents",
    "toc",
    "tocfill",
    "toc_fill"
  ],
  "author": "John Haugeland <stonecypher@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StoneCypher/txt_tocfill/issues"
  },
  "homepage": "https://github.com/StoneCypher/txt_tocfill#readme",
  "devDependencies": {
    "ava": "^0.19.1",
    "ava-spec": "^1.1.0",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^14.3.0",
    "rimraf": "^2.6.1"
  }
}
